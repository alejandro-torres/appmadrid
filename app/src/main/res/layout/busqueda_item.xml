<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="10dp">

    <TextView
        android:id="@+id/textView_tituloBusqueda"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="TextView"
        android:textColor="@android:color/black"
        android:textStyle="bold"
        card_view:layout_constraintStart_toStartOf="parent"
        card_view:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_precioBusqueda"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        card_view:layout_constraintStart_toStartOf="@+id/textView_tituloBusqueda"
        card_view:layout_constraintTop_toBottomOf="@+id/textView_distritoBusqueda" />

    <TextView
        android:id="@+id/textView_distritoBusqueda"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="TextView"
        card_view:layout_constraintStart_toStartOf="@+id/textView_tituloBusqueda"
        card_view:layout_constraintTop_toBottomOf="@+id/textView_tituloBusqueda" />

    <TextView
        android:id="@+id/textView_fechaBusqueda"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:text="TextView"
        card_view:layout_constraintStart_toEndOf="@+id/textView_precioBusqueda"
        card_view:layout_constraintTop_toTopOf="@+id/textView_precioBusqueda" />

    <ImageView
        android:id="@+id/imageView_favoritoOff"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        card_view:layout_constraintBottom_toBottomOf="@+id/textView_precioBusqueda"
        card_view:layout_constraintEnd_toEndOf="parent"
        card_view:layout_constraintHorizontal_bias="0.958"
        card_view:layout_constraintStart_toStartOf="parent"
        card_view:layout_constraintTop_toTopOf="@+id/textView_tituloBusqueda"
        card_view:layout_constraintVertical_bias="0.463"
        card_view:srcCompat="@drawable/ic_estrella_sinmarcar_amarilla_24dp" />

    <ImageView
        android:id="@+id/imageView_favoritoOn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        card_view:layout_constraintBottom_toBottomOf="@+id/textView_precioBusqueda"
        card_view:layout_constraintEnd_toEndOf="parent"
        card_view:layout_constraintHorizontal_bias="0.958"
        card_view:layout_constraintStart_toStartOf="parent"
        card_view:layout_constraintTop_toTopOf="@+id/textView_tituloBusqueda"
        card_view:layout_constraintVertical_bias="0.463"
        card_view:srcCompat="@drawable/ic_estrella_marcada_amarilla_24dp" />

    <TextView
        android:id="@+id/textView_fechaFinBusqueda"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="TextView"
        card_view:layout_constraintBottom_toBottomOf="@+id/textView_precioBusqueda"
        card_view:layout_constraintStart_toEndOf="@+id/textView_guionFechaBusqueda"
        card_view:layout_constraintTop_toTopOf="@+id/textView_precioBusqueda"
        card_view:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView_guionFechaBusqueda"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="@string/guion_fecha"
        card_view:layout_constraintBottom_toBottomOf="@+id/textView_fechaBusqueda"
        card_view:layout_constraintStart_toEndOf="@+id/textView_fechaBusqueda"
        card_view:layout_constraintTop_toTopOf="@+id/textView_fechaBusqueda" />

</androidx.constraintlayout.widget.ConstraintLayout>